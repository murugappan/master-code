                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Country__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Country</label>
        <referenceTo>CountryObject__c</referenceTo>
        <relationshipLabel>States</relationshipLabel>
        <relationshipName>States</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>FullIsoCode__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>Country__r.IsoCode_2__c + &apos;-&apos; + IsoCode__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Full ISO Code</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>IsoCode__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>ISO Code</label>
        <length>3</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Notes__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Notes</label>
        <length>32000</length>
        <type>LongTextArea</type>
        <visibleLines>5</visibleLines>
    </fields>
    <fields>
        <fullName>RegionalDivision__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Regional Division</label>
        <length>3</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>TrimmedName__c</fullName>
        <deprecated>false</deprecated>
        <description>Takes the first 20 characters of the state name which ensures the right length for inserting into standard state fields.</description>
        <externalId>false</externalId>
        <formula>TRIM ( LEFT (Name, 20) )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Trimmed Name</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
