    <description>A list of standard States/Counties/Provinces used by CountryComplete for both auto-completion at the UI level and also for validation and standardization and the database level.  All States have a parent Country object</description>
    <enableActivities>false</enableActivities>
    <enableFeeds>false</enableFeeds>
    <enableHistory>false</enableHistory>
    <enableReports>false</enableReports>
    <fields>
        <fullName>Category__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Category</label>
        <picklist>
            <picklistValues>
                <fullName>County</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Province</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
